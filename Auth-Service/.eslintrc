{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "rules": {
    "eqeqeq": "off",
    "no-console": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "no-param-reassign": "off",
    "arrow-body-style": "off",
    "import/prefer-default-export": "off",
    "import/no-extraneous-dependencies": "off",
    "camelcase": "off"
  }
}